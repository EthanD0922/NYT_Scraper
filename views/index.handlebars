<div>
    {{#each data}}
        <div class= "card">
            <div class="card-header">
                {{this.title}}
            </div>
            <div class="card-body">
                <div class="card-text">{{this.summary}}</div>
                <a href = "{{this.link}}" class="btn btn-primary">Go To Page</a>
            </div>
        </div>
    {{/each}}
</div>