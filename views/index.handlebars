<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">Navbar</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        <a class="nav-link" href="/">Home</a>
      </li>
      <li class="nav-item">
        <button class="saved btn btn-warning">Saved</button>
      </li>
      <li class="nav-item">
        <button class="scrape btn btn-warning ml-2">Scrape</button>
      </li>
    </ul>
  </div>
</nav>

<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">NYT Scraper</h1>
    <p class="lead">Scrape an Article Below</p>
  </div>
</div>



<div>
    {{#each data}}                
        <div class="accordion mx-4" id="accordionExample">
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h2 class="mb-0">
                        <button class="btn btn-link collapsed" type="button" data-toggle="collapse" data-target="#collapse{{this._id}}" aria-expanded="true" aria-controls="collapseOne">
                        {{this.title}}
                        </button>
                    </h2>
                </div>

                    <div id="collapse{{this._id}}" class="collapse" aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        {{this.summary}}
                    </div>
                    <a href = "http://www.nytimes.com{{this.link}}" class="btn btn-primary m-2">Go To Page</a>
                </div>
                </div>
        </div>
    {{/each}}
</div>

<footer class="footer">
  <div class="container">
    <p>New York Times Mongo Scraper</p>
  </div>
</footer>